# Lysobacter Database Environment Configuration
# Copy this file to .env and adjust values as needed

# Database Configuration
POSTGRES_DB=lysobacter_db
POSTGRES_USER=lysobacter_user
POSTGRES_PASSWORD=lysobacter_password

# Database Connection (for external tools)
DB_HOST=localhost
DB_PORT=5434
DB_NAME=lysobacter_db
DB_USER=lysobacter_user
DB_PASSWORD=lysobacter_password

# Container Configuration
CONTAINER_NAME=lysobacter_postgres
POSTGRES_VERSION=latest

# Data Volume
POSTGRES_DATA_PATH=./data/postgres 